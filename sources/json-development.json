x-speakeasy-retries:
  backoff:
    exponent: 1.5
    initialInterval: 500
    maxElapsedTime: 3600000
    maxInterval: 30000
  retryConnectionErrors: true
  statusCodes:
    - "408"
    - "409"
    - "429"
    - 5XX
  strategy: backoff
